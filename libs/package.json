{
  "name": "microapp-libs",
  "version": "1.0.0",
  "description": "Libraries used by the MicroApp App and all its MicroApps",
  "main": "dist/microapp-libs.js",
  "main-dev": "../static/js/microapps/microapp-libs.js",
  "scripts": {
    "postbuild": "node scripts/postbuild.js",
    "build": "rollup -c",
    "start": "NODE_ENV=development rollup -c -w"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "@rollup/plugin-json": "^4.0.3",
    "babel-loader": "^8.1.0",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-re": "^1.0.7",
    "rollup-plugin-terser": "^5.3.1",
    "rollup-plugin-url": "^2.2.4"
  },
  "dependencies": {
    "@use-it/event-listener": "^0.1.3"
  },
  "files": [
    "dist"
  ],
  "babel": {
    "presets": [
      [
        "@babel/env"
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
}